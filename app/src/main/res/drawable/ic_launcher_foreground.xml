<?xml version="1.0" encoding="utf-8"?>
<!--
    Vector drawable for StackApp launcher icon foreground
    Displays a stack symbol (three horizontal bars)
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Background circle -->
    <path
        android:fillColor="#6366F1"
        android:pathData="M54,54m-40,0a40,40 0,1 1,80 0a40,40 0,1 1,-80 0"/>
    
    <!-- Stack bars (three stacked rectangles) -->
    <!-- Bottom bar -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M30,62L78,62L78,70L30,70Z"/>
    
    <!-- Middle bar -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M35,50L73,50L73,58L35,58Z"/>
    
    <!-- Top bar (ToS) -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M40,38L68,38L68,46L40,46Z"/>
    
    <!-- Arrow pointing to top -->
    <path
        android:fillColor="#22C55E"
        android:pathData="M54,28L62,36L46,36Z"/>
    
</vector>

